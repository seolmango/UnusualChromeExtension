{
  "manifest_version": 3,
  "name": "offnamuwiki",
  "version": "1.0",
  "description": "꺼무위키나라",
  "content_scripts": [
    {
      "matches": ["https://namu.wiki/*"],
      "js": ["index.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
}